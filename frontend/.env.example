# Example environment variables for the frontend
# Copy to .env and adjust values as needed

# Base URL to the user-service HTTP API (must include /api suffix)
NEXT_PUBLIC_USER_SERVICE_URL=http://user-service:7001/api

# Socket.IO server URL for realtime events
NEXT_PUBLIC_SOCKET_URL=http://realtime-service-ts:8080

# Default page size for paginated lists
NEXT_PUBLIC_PAGE_SIZE=20
